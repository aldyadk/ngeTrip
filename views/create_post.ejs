<!DOCTYPE html>
<html>
<head>
  <title>nge-TRIP</title>
  <link href="https://fonts.googleapis.com/css?family=Abel|Amatic+SC|Raleway:100,200,300,400,500,600,700,800,900" rel="stylesheet">
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>

  <div class="navigation">
    <ul>
      <li>
        <form action="/" method="GET">
          <input type="submit" value="Beranda">
        </form>
      </li>
      <li id="logo">nge-Trip</li>
      <li>
        <form action="/post" method="POST">
          <input type="submit" value="Daftar Postingan">
        </form>
      </li>
    </ul>
  </div>

  <div class="create_post">
    <div class="products">
      <div class="form-style-6">
      <p class="text_update">Buat Post Baru</p>
      <form method="post" action="/post/create">
      <input type="text" name="title" placeholder="Topik" />
      <textarea name="description" placeholder="Masukkan deskripsi topik anda"></textarea>
      <select name="placeId">
        <% dataPlaces.forEach(place => {%>
        <option value="<%- place.id%>" data-icon="<%- place.imagePath%>"><%- place.placeName%></option>
        <%})%>
      </select>
      <input type="submit"name="sumbit" value="Create" />
      </form>
      </div>
    </div>
  </div>

  <h3>Mari bersama-sama memajukan sektor pariwisata Indonesia</h3>

</body>
</html>